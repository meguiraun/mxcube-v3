<?xml version="1.0" encoding="iso-8859-1"?>
<beamline_pars>
  <COLLECT_PARS>
    <fileinfo>
      <directory>/data/data1</directory>
      <prefix>user_</prefix>
      <run_number>1</run_number>
    </fileinfo>
    <current_filename></current_filename>
    <dark>1</dark>
  </COLLECT_PARS>
  <SPEC_PARS>
    <phi>phi</phi>
    <wavmot>lambda</wavmot>
    <emot>energy</emot>
    <detdistmot>dtox</detdistmot>
    <beam>
	<ax>0.00319602599266</ax>
	<ay>0.00186802943976</ay>
	<bx>113.830831497</bx>
	<by>108.539965208</by>
    </beam>
    <beamsize>
      <!-- determined by apertures -->
      <max_hor>0.080</max_hor>
      <max_ver>0.030</max_ver>
    </beamsize>

    <detector>
      <binning>1</binning>
 <!--     <TacoDeviceName>id14/adsc/2</TacoDeviceName> -->
      <save_raw>0</save_raw>
      <no_xform>0</no_xform>
      <DarkTime>1</DarkTime>
      <UseDefaultDarkTime>1</UseDefaultDarkTime>
    </detector>
    <maximum_counts>2</maximum_counts>
    <intensity_calibration_constant>1</intensity_calibration_constant>
  </SPEC_PARS>
  <BCM_PARS>
    <synchrotron_name>MAXLAB</synchrotron_name>
    <undulator>
           <type>u35u</type>
           <device_uri>id/id/29</device_uri>
           <gap_index>0</gap_index>
    </undulator>
    <undulator>
           <type>u21d</type>
           <device_uri>id/id/29</device_uri>
           <gap_index>2</gap_index>
    </undulator>
    <monochromator_type>channel cut</monochromator_type>
    <diffractometer>
      <type>md2</type>
    </diffractometer>
    <detector>
      <type>MAR</type>
      <model>marmosaic</model>
    </detector>
    <detector_radius>112.5</detector_radius>
    <pixel_size_x>0.07324</pixel_size_x>
    <pixel_size_y>0.07324</pixel_size_y>
    <FileSuffix>mccd</FileSuffix>
    <monochromator>Si(111)</monochromator>
    <focusing_optic>Toroidal mirror</focusing_optic>
    <curr_wavelength>1.0</curr_wavelength>
    <tunable_wavelength>True</tunable_wavelength>
    <beam_shape>circular</beam_shape>
    <polarisation>0.99</polarisation>
    <beam_divergence_vertical>6.5</beam_divergence_vertical>
    <beam_divergence_horizontal>104</beam_divergence_horizontal>
    <default_exposure_time>3.0</default_exposure_time>
    <maximum_exposure>200.0</maximum_exposure>
    <minimum_exposure_time>0.05</minimum_exposure_time>
    <minimum_phi_speed>0.01</minimum_phi_speed>
    <minimum_phi_oscillation>0.01</minimum_phi_oscillation>
    <maximum_phi_speed>100</maximum_phi_speed>
    <maximum_radiation_exposure>200</maximum_radiation_exposure>
    <default_exposure_time>5.0</default_exposure_time>
    <default_number_of_passes>1</default_number_of_passes>
    <beam>
	<ax>0.00319602599266</ax>
	<ay>0.00186802943976</ay>
	<bx>113.830831497</bx>
	<by>108.539965208</by>
    </beam>
    <hutch>bli9113</hutch>
  </BCM_PARS>
 <VALIDATE_PARS>
    <exposure_time>0.05,100</exposure_time>
    <start_angle>-1000,1000</start_angle>
    <range>0.0,10</range>
    <number_of_passes>1,10</number_of_passes>
    <start_image_number>0,10000</start_image_number>
    <run_number>0,10000</run_number>
    <overlap>-90,0.99</overlap>
    <number_of_images>1,10000</number_of_images>
 </VALIDATE_PARS>
 <EXTRAVALIDATE_PARS>
    <rotation_speed>0.01,100</rotation_speed>
 </EXTRAVALIDATE_PARS>
 <INHOUSE_USERS>
   <proposal>
     <code>opid</code>
     <number>141</number>
   </proposal>
   <proposal>
     <code>mxihr</code>
   </proposal>
 </INHOUSE_USERS>

</beamline_pars>
